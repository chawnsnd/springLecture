<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="wiki.bbongnamu.WikiMapper">

	<insert id="insertWikiHistory" parameterType="wikiHistory">
		insert into tbl_wiki(
			num,
			wikinum,
			usernum,
			explanation
		)values(
			seq_wiki_history.nextval,
			#{wikinum},
			#{usernum},
			#{explanation}
		)
	</insert>
	
	<select id="selectWikiHistory" resultType="wikiHistory">
		select *
		from tbl_wiki
		where num = #{num}
	</select>

	<select id="selectWikiHistoryByWikinum" resultType="wikiHistory">
		select *
		from tbl_wiki
		where wikinum = #{wikinum}
	</select>

</mapper>